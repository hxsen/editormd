{
    "name": "hxsen/laravel-admin-editormd",
    "description": "EditorMD extension for laravel-admin",
    "type": "library",
    "keywords": ["laravel-admin", "extension", "laravel-admin-editormd"],
    "homepage": "https://github.com/hxsen/laravel-admin-editormd",
    "license": "MIT",
    "authors": [
        {
            "name": "hxsen",
            "email": "hx@hxsen.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "~1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Hxsen\\EditorMd\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hxsen\\EditorMd\\EditorMdServiceProvider"
            ]
        }
    }
}
